{
	"name": "updater-front",
	"version": "0.1.0",
	"private": true,
	"homepage": "/updater/",
	"scripts": {
		"start": "craco start",
		"build": "craco build",
		"docker": "node ../deploy/build/build.js",
		"refresh-clients": "cd scripts/RefreshApis && dotnet run"
	},
	"dependencies": {
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@mui/icons-material": "^5.11.16",
		"@mui/material": "^5.12.0",
		"@mui/styles": "^5.12.0",
		"@reduxjs/toolkit": "^1.9.3",
		"@swc/helpers": "^0.5.0",
		"axios": "^1.3.5",
		"inversify": "^6.0.1",
		"inversify-react": "^1.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "8.0.5",
		"react-toastify": "^9.1.2",
		"redux": "4.2.1",
		"reflect-metadata": "^0.1.13",
		"semver": "^7.5.2"
	},
	"devDependencies": {
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint": "^8.38.0",
		"@craco/craco": "^7.1.0",
		"@types/node": "^18.15.11",
		"@types/react": "^18.0.35",
		"@types/react-dom": "^18.0.11",
		"@types/react-redux": "^7.1.25",
		"craco-swc": "^0.5.1",
		"react-scripts": "^5.0.1",
		"sass": "^1.62.0",
		"typescript": "^5.0.4",
		"prettier": "^2.8.7"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"plugin:prettier/recommended",
			"eslint:recommended"
		],
		"plugins": [
			"prettier"
		],
		"rules": {
			"prettier/prettier": "error",
			"no-mixed-spaces-and-tabs": "off"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
